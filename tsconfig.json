{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ESNext",
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "allowJs": true,
    "baseUrl": "src",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "paths": {
      "@/*": ["src/*"]
    },
    "lib": ["esnext", "dom", "dom.iterable", "ES6"],
    "sourceMap": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "outDir": "build",
    "strict": false,
    "strictNullChecks": true
  },
  "include": [
    "src",
    "webpack.config.ts",
    "__tests__/Login.test.ts",
    "public/OneSignalSDKWorker.js"
  ]
}
