  "description": "Elevvate Technologies manages Cuttinboard, an application to help restaurants be more organized and intercommunicated. Cuttinboard is the platform where restaurant professionals perform the digital work. Cuttinboard brings restaurant employees into a centralized platform of information exchange.",
  "scripts": {
    "start": "webpack-cli serve --mode=development --env development --open --hot",
    "build": "webpack --mode=production --env production --progress",
    "build:all": "npm run fetch:i18n && webpack --mode=production --env production --progress",
    "lint": "eslint src/**/*.{ts,tsx} --quiet --fix",
    "publish": "npm run build && firebase deploy --only hosting",
    "publish-preview": "npm run build && firebase hosting:channel:deploy preview --expires 7d",
    "test": "jest --coverage",
    "push:i18n": "node push-google-spreadsheet.js",
    "fetch:i18n": "node fetch-google-spreadsheet.js",
    "deploy-rules": "firebase deploy --only firestore:rules",
    "deploy-rules:database": "firebase deploy --only database:rules",
    "deploy-rules:storage": "firebase deploy --only storage",
    "update-cuttinboard-library": "npm i @rodez97/cuttinboard-library@latest"
  },
